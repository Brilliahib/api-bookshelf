generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Book{
  id String @id @default(cuid())
  name String
  year DateTime
  author String
  summary String
  publisher String
  pageCount Int
  readPage Int
  finished Boolean
  reading Boolean
  insertedAt DateTime
  updatedAt DateTime
}